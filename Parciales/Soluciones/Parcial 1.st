Object subclass: #Equipo	instanceVariableNames: 'denominacion categoria personas'	classVariableNames: ''	package: '90328_WedemeyerFederico'!!Equipo methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:10'!categoria	^ categoria! !!Equipo methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:10'!denominacion: anObject	denominacion := anObject! !!Equipo methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:10'!personas: anObject	personas := anObject! !!Equipo methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:10'!personas	^ personas! !!Equipo methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:10'!categoria: anObject	categoria := anObject! !!Equipo methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:10'!denominacion	^ denominacion! !!Equipo methodsFor: 'initialization' stamp: 'FedeWedemeyer 4/30/2022 11:11'!initialize	categoria:= ''.	denominacion:= ''.	personas:= OrderedCollection new.! !!Equipo methodsFor: 'control' stamp: 'FedeWedemeyer 4/30/2022 11:43'!agregarPersona: unaPersona	personas add: unaPersona.	! !!Equipo methodsFor: 'control' stamp: 'FedeWedemeyer 4/30/2022 11:49'!listadoMasValorados: unaValoracion|lista string|	string:= 'La lista de jugadores mas valorados es: '.		lista:= personas reject: [ :x| x localidad = 'Cordoba' and: x valoracion < unaValoracion].		lista do: [ :x| string:= string, x asString, String cr. ].	^string.	! !!Equipo methodsFor: 'control' stamp: 'FedeWedemeyer 4/30/2022 11:58'!jugadoresConFaltas: limiteInferior hasta: limiteSuperior|res|	res:=	personas select: [:x| ((x isKindOf: Jugador) and: (x estaSuspendido)) and: (x faltas between: limiteInferior and: limiteSuperior). ].		^res.! !!Equipo methodsFor: 'control' stamp: 'FedeWedemeyer 4/30/2022 12:02'!grupoSanguineo|grupoAB grupoB msg|msg:= 'Son mas jugadores con grupo sanguineo B-'.	grupoAB:= personas select: [ :x | x grupoSanguineo = 'AB+' ].	grupoB:= personas select: [ :x | x grupoSanguineo= 'B-' ].		(grupoAB size) > (grupoB size)	ifTrue: [ msg:= 'Son msa jugadores con grupo sanguineo AB+' ].^msg! !!Equipo methodsFor: 'control' stamp: 'FedeWedemeyer 4/30/2022 12:30'!mayorGrupoSanguineo|grupoAB grupoB msg|	msg:= ''.	grupoAB:= personas select: [ :x | x grupoSanguineo = 'AB+' ].	grupoB:= personas select: [ :x | x grupoSanguineo = 'B-' ].		(grupoAB size) > (grupoB size)	ifTrue: [ msg:= 'Son mas jugadores con grupo sanguineo AB+'. ]	ifFalse: [ msg:= 'Son mas jugadores con grupo sanguineo B-'.].	^msg! !Equipo subclass: #Persona	instanceVariableNames: 'nombre apellido localidad grupoSanguineo'	classVariableNames: ''	package: '90328_WedemeyerFederico'!!Persona methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:13'!nombre: anObject	nombre := anObject! !!Persona methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:13'!localidad: anObject	localidad := anObject! !!Persona methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:13'!apellido	^ apellido! !!Persona methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:13'!nombre	^ nombre! !!Persona methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:13'!grupoSanguineo: anObject	grupoSanguineo := anObject! !!Persona methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:13'!localidad	^ localidad! !!Persona methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:13'!grupoSanguineo	^ grupoSanguineo! !!Persona methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:13'!apellido: anObject	apellido := anObject! !!Persona methodsFor: 'control' stamp: 'FedeWedemeyer 4/30/2022 11:14'!valoracion^self subclassResponsibility.! !!Persona methodsFor: 'control' stamp: 'FedeWedemeyer 4/30/2022 11:18'!asString |string|	string:= 'Apellido: ', apellido asString, ' | Nombre: ', nombre asString, ' | Localidad: ', localidad asString, ' | Grupo Sanguineo: ', grupoSanguineo asString.	^string.! !!Persona methodsFor: 'initialization' stamp: 'FedeWedemeyer 4/30/2022 12:10'!initialize	super initialize.	apellido:= ''.	grupoSanguineo:= ''.	localidad:= ''.	nombre:= ''.! !Persona subclass: #Entrenador	instanceVariableNames: 'cargo antiguedad'	classVariableNames: ''	package: '90328_WedemeyerFederico'!!Entrenador methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:16'!antiguedad: anObject	antiguedad := anObject! !!Entrenador methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:16'!cargo	^ cargo! !!Entrenador methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:16'!cargo: anObject	cargo := anObject! !!Entrenador methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:16'!antiguedad	^ antiguedad! !!Entrenador methodsFor: 'initialization' stamp: 'FedeWedemeyer 4/30/2022 12:10'!initialize	super initialize.	antiguedad:= 0.	cargo:= ''.	! !!Entrenador methodsFor: 'control' stamp: 'FedeWedemeyer 4/30/2022 12:24'!valoracion|val|	val:= 0.		(self antiguedad > 3)	ifTrue: [ val:= 8.		self antiguedad > 10		ifTrue: [ val:= 8 + ((antiguedad - 10) * 2)]		]	ifFalse: [ val:= 5 ].	^val.! !!Entrenador methodsFor: 'control' stamp: 'FedeWedemeyer 4/30/2022 11:17'!asString|string|string:= super asString, 'Cargo: ', cargo asString, ' | Antiguedad: ', antiguedad asString.^string.! !Persona subclass: #Jugador	instanceVariableNames: 'posicion faltas goles'	classVariableNames: ''	package: '90328_WedemeyerFederico'!!Jugador methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:19'!faltas	^ faltas! !!Jugador methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:19'!posicion	^ posicion! !!Jugador methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:19'!goles: anObject	goles := anObject! !!Jugador methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:19'!goles	^ goles! !!Jugador methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:19'!faltas: anObject	faltas := anObject! !!Jugador methodsFor: 'accessing' stamp: 'FedeWedemeyer 4/30/2022 11:19'!posicion: anObject	posicion := anObject! !!Jugador methodsFor: 'initialization' stamp: 'FedeWedemeyer 4/30/2022 12:10'!initialize	super initialize.	posicion:= ''.	faltas:= 0.	goles:= 0.! !!Jugador methodsFor: 'control' stamp: 'FedeWedemeyer 4/30/2022 11:40'!estaSuspendido	^faltas > 4.! !!Jugador methodsFor: 'control' stamp: 'FedeWedemeyer 4/30/2022 12:24'!valoracion|val|val:= 0.	goles > 5	ifTrue: [ goles > 10 ifTrue: [val:= goles raisedTo: 2.]		ifFalse: [ val:= (goles * 2) - faltas ].]		ifFalse: [ val:= goles ].		 ^val.		! !!Jugador methodsFor: 'control' stamp: 'FedeWedemeyer 4/30/2022 11:42'!asString |string|	string:= 'Faltas: ', faltas asString, ' | Goles: ', goles asString, ' | Posicion: ' posicion asString.	^string.! !